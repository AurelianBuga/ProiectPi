<Window x:Class="Proiect_PI.AddLinkInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proiect_PI"
        mc:Ignorable="d"
        Title="AddLinkInfo" Height="300" Width="438.559">
    <Grid>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="15,10,0,0" Text="Alias for Link"  TextWrapping="Wrap" VerticalAlignment="Top" Width="160"/>
        <TextBox x:Name="aliaTextBox" HorizontalAlignment="Left" Height="23" Margin="15,39,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="alias_TextChanged" Text="Alias..." GotFocus="aliaTextBox_GotFocus"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="15,72,0,0" TextWrapping="Wrap" Text="Text" VerticalAlignment="Top"/>
        <TextBox x:Name="LinkTextBox" HorizontalAlignment="Left" Height="139" Margin="10,104,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="399" TextChanged="text_TextChanged"/>
        <Button x:Name="AddButton" Content="Add" HorizontalAlignment="Left" Margin="283,39,0,0" VerticalAlignment="Top" Width="75" Height="42" Click="AddButton_Click"/>
    </Grid>
</Window>
